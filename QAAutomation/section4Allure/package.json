{
  "name": "a1qa-wdio-framework",
  "type": "module",
  "scripts": {
    "chrome.mocha": "wdio run ./configs/chrome.mocha.conf.js",
    "test:alert": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/AlertTest.js",
    "test:data:tables": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/DataTablesTest.js",
    "test:dynamic:controls": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/DynamicControlsTest.js",
    "test:file:downloaded": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/FileDownloadedTest.js",
    "test:file:uploaded": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/FileUploadedTest.js",
    "test:iframe": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/IFrameTest.js",
    "test:ui:dynamic:tables": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/UITestAutomationPlaygroundTest.js",
    "test:ui:verify:text": "wdio run ./configs/chrome.mocha.conf.js --spec ./test/specs/UIVerifyTextTest.js"
  },
  "dependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/register": "^7.23.7",
    "@wdio/allure-reporter": "^9.1.2",
    "@wdio/cli": "^8.35.1",
    "@wdio/local-runner": "^8.35.1",
    "@wdio/mocha-framework": "^8.35.0",
    "@wdio/spec-reporter": "^8.32.4",
    "allure-js-commons": "^3.0.4",
    "chai": "^5.1.0",
    "fs-extra": "^11.2.0",
    "winston": "^3.13.0"
  }
}
