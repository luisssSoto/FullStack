        "@wdio/cli": "^8.35.1",
        "@wdio/cucumber-framework": "^9.1.3",
        "@wdio/local-runner": "^8.35.1",
        "@wdio/mocha-framework": "^8.35.0",
        "@wdio/spec-reporter": "^8.32.4",
        "allure-commandline": "^2.30.0",
        "allure-js-commons": "^3.0.4",
